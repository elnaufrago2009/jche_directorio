#region-tiked_niveles.container
  .region
    .page-header
      %h1= page_title!('Tiked Nivel')

  .region
    %p
      %b Nombre:
      = @tiked_nivele.nombre
    %p
      %b Descripcion:
      = @tiked_nivele.descripcion

  .region
    .admin-controls
      = link_to '<span class="glyphicon glyphicon-edit"></span> Editar'.html_safe, edit_tiked_nivele_path(@tiked_nivele), class: 'btn btn-success btn-sm'
      = link_to '<span class="glyphicon glyphicon-arrow-left"></span> Regresar'.html_safe, tiked_niveles_path, class: 'btn btn-warning btn-sm'
